package browser;

import org.openqa.selenium.WebDriver;
import org.openqa.selenium.chrome.ChromeDriver;

public class Open_Browser {
	
	public static void main(String[] args) {
		
		//System.setProperty("webdriver.chrome.driver", "C:\\Users\\Dell\\eclipse-workspace3\\Selenium_First_Class\\Drivers\\chromedriver.exe");
		System.setProperty("webdriver.chrome.driver", "./Selenium_First_Class/Drivers/chromedriver.exe");
		
		
		WebDriver driver = new ChromeDriver();
		driver.get("https://selenium.dev/downloads/");
		driver.close();
		driver.quit();
		
	}

}
